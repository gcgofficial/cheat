--MENU ADD IN SECRIPT
HOME = 1
function HOME()
multi = gg.multiChoice({
"[] CAMERA TENGAH/YOUTUBER ",
"[] DHOLPIN SET MANUAL ",
"[] SPEEP KHUSUS BALAP ",
"EXIT",

},nil,'BUSSID~V3.7.1')

if multi == nil then else
if multi[1] == true then ch1() end
if multi[2] == true then ch2() end
if multi[3] == true then ch3() end
if multi[4] == true then os.exit()
end
end
HOMEDM = -1
end

function ch1()
gg.alert('GCG~OFFICIAL~MOD~MENU~',"menu.1_proses")
gg.searchNumber("0.69999998808;5.19999980927:9", ggTYPE_FLOAT)
--[[ found: 0 ]]
gg.refineNumber("0.69999998808", gg.TYPE_FLOAT)
--[[ found: 12 ]]
gg.getResults(10)
--[[count: 10 ]]
gg.editAll("0",gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("2.25;5.19999980927:9", ggTYPE_FLOAT)
--[[ found: 0 ]]
gg.refineNumber("2.25", gg.TYPE_FLOAT)
--[[ found: 12 ]]
gg.getResults(10)
--[[count: 10 ]]
gg.editAll("2.23",gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("2.23;5.19999980927:9", ggTYPE_FLOAT)
--[[ found: 0 ]]
gg.refineNumber("5.19999980927", gg.TYPE_FLOAT)
--[[ found: 12 ]]
gg.getResults(10)
--[[count: 10 ]]
gg.editAll("4",gg.TYPE_FLOAT)
gg.toast("menu.1")
gg.clearResults()
gg.setVisible(false)
end

