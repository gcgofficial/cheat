function Exit()
print ("HATUR NUHUN")
gg.toast("SCRIPT BY GCG")
gg.skipRestoreState()
  os.exit()
  gg.setVisible(true)
end

while true do
  if gg.isVisible(true) then
    PUBGMH = 1
    gg.setVisible(false)
  end
  gg.clearResults()
  if PUBGMH == 1 then
    HOME()
  end
end

