function HOME()
TRD = gg.multiChoice({
	"1C➡️➡️➡️0,001",
	"1C➡️➡️➡️0,01",
	"1C➡️➡️➡️0,00001",
	"88⬅️🔛➡️,25",
	"88➡️🔛➡️0,85",
	"88➡️🔛➡️0,55",
	"94◀️🔼▶️0,87",
	"94◀️🔼▶️0,75",
	"94◀️🔼▶️0,99",
	"A0↔️🔼↔️0,25",
	"A0↔️🔼↔️0,85",
	"A0↔️🔼↔️0,55",
	"10🔼⏏️🔼0,40",
	"10🔼⏏️🔼0,66",
	"2C⏏️⏏️⏏️-0,22",
	"2C⏏️⏏️⏏️-0,66",
	"📍OPTIMAL📍",
	"EXIT",
}, nil, ":::BANGKALAN:::SCRIPT  BY GCG TEKAN OK ")
if TRD == nil then else
if TRD [1] == true then A1()end
if TRD [2] == true then A2()end
if TRD [3] == true then A3()end
if TRD [4] == true then A11()end
if TRD [5] == true then A12()end
